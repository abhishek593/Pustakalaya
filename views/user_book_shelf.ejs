<%- include('partials/header') %>
    
<table>
    <thead>
        <tr>
            <td>S.No</td>
            <td>ISBN</td>
            <td>Title</td>
        </tr>
        <% for(var i=1;i<=results.length;i++){   %>
            <tr>
                <td><%= i %></td>
                <td><%= results[i-1].ISBN %></td>
                <td><%= results[i-1].title %></td>
            </tr>
        <% }    %>
    </thead>
</table>

